# teamID=1 の場合
execute as @a[scores={respawnTimer=0},tag=spConfigured] if score @s teamID matches 1 run gamemode survival @s
execute as @a[scores={respawnTimer=0},tag=spConfigured] if score @s teamID matches 1 run tp @s @e[type=armor_stand,tag=spawn_team_1,limit=1,sort=nearest]
execute as @a[scores={respawnTimer=0},tag=spConfigured] if score @s teamID matches 1 run execute at @s run tp @s ~ ~-500 ~
execute as @a[scores={isDead=1..},tag=spConfigured] if score @s teamID matches 1 run scoreboard players reset @s isDead

